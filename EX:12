#include <stdio.h>
#include <ctype.h>

char stack[100];
int top=-1;

void push(char c){ stack[++top]=c; }
char pop(){ return stack[top--]; }
int priority(char c){
    if(c=='+'||c=='-') return 1;
    if(c=='*'||c=='/') return 2;
    return 0;
}

int main(){
    char exp[100], c;
    int i;
    printf("Enter Infix Expression: ");
    scanf("%s",exp);

    for(i=0;exp[i]!='\0';i++){
        if(isalnum(exp[i]))
            printf("%c",exp[i]);
        else{
            while(top!=-1 && priority(stack[top])>=priority(exp[i]))
                printf("%c",pop());
            push(exp[i]);
        }
    }

    while(top!=-1)
        printf("%c",pop());

    return 0;
}
