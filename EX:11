#include <stdio.h>
#define MAX 100

int stack[MAX], top = -1;

void push(int val){
    if(top == MAX-1){
        printf("Stack Overflow\n");
        return;
    }
    stack[++top] = val;
}

void pop(){
    if(top == -1){
        printf("Stack Underflow\n");
        return;
    }
    printf("Popped: %d\n", stack[top--]);
}

void peek(){
    if(top != -1)
        printf("Top Element: %d\n", stack[top]);
}

int main(){
    push(10);
    push(20);
    push(30);
    peek();
    pop();
    peek();
    return 0;
}
